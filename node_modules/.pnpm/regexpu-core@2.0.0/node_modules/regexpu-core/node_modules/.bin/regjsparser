#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regjsparser@0.1.5/node_modules/regjsparser/bin/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regjsparser@0.1.5/node_modules/regjsparser/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regjsparser@0.1.5/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules:/home/workspace/react-hooks/coding-practices/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regexpu-core@2.0.0/node_modules/regexpu-core/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regexpu-core@2.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regjsparser@0.1.5/node_modules/regjsparser/bin/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regjsparser@0.1.5/node_modules/regjsparser/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regjsparser@0.1.5/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules:/home/workspace/react-hooks/coding-practices/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regexpu-core@2.0.0/node_modules/regexpu-core/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/regexpu-core@2.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../regjsparser@0.1.5/node_modules/regjsparser/bin/parser" "$@"
else
  exec node  "$basedir/../../../../../regjsparser@0.1.5/node_modules/regjsparser/bin/parser" "$@"
fi
