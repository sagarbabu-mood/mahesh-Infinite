#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/upgrader/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/husky@4.3.8/node_modules/husky/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/husky@4.3.8/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules:/home/workspace/react-hooks/coding-practices/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/husky/lib/upgrader/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/husky/lib/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/husky/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/upgrader/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/husky@4.3.8/node_modules/husky/lib/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/husky@4.3.8/node_modules/husky/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/husky@4.3.8/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules:/home/workspace/react-hooks/coding-practices/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/husky/lib/upgrader/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/husky/lib/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/husky/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../husky/lib/upgrader/bin.js" "$@"
else
  exec node  "$basedir/../husky/lib/upgrader/bin.js" "$@"
fi
