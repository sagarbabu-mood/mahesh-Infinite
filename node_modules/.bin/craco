#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/@craco+craco@6.1.1_react-scripts@4.0.3/node_modules/@craco/craco/bin/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/@craco+craco@6.1.1_react-scripts@4.0.3/node_modules/@craco/craco/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/@craco+craco@6.1.1_react-scripts@4.0.3/node_modules/@craco/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/@craco+craco@6.1.1_react-scripts@4.0.3/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules:/home/workspace/react-hooks/coding-practices/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/@craco/craco/bin/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/@craco/craco/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/@craco/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/@craco+craco@6.1.1_react-scripts@4.0.3/node_modules/@craco/craco/bin/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/@craco+craco@6.1.1_react-scripts@4.0.3/node_modules/@craco/craco/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/@craco+craco@6.1.1_react-scripts@4.0.3/node_modules/@craco/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/@craco+craco@6.1.1_react-scripts@4.0.3/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/.pnpm/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules:/home/workspace/react-hooks/coding-practices/node_modules:/home/workspace/react-hooks/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/@craco/craco/bin/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/@craco/craco/node_modules:/home/workspace/react-hooks/coding-practices/readMoreLess/node_modules/@craco/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@craco/craco/bin/craco.js" "$@"
else
  exec node  "$basedir/../@craco/craco/bin/craco.js" "$@"
fi
